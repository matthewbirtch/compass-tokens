{
  "$schema": "https://tr.designtokens.org/format/",
  "color": {
    "theme": {
      "denim": {
        "center-channel-bg": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "center-channel-color": {
          "$type": "color",
          "$value": "{color.foundation.neutral.900}"
        },
        "sidebar-bg": {
          "$type": "color",
          "$value": "{color.foundation.indigo.500}"
        },
        "sidebar-header-bg": {
          "$type": "color",
          "$value": "#192A4D",
          "$description": "Custom value between indigo shades"
        },
        "sidebar-text": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "sidebar-text-hover-bg": {
          "$type": "color",
          "$value": "{color.foundation.indigo.400}"
        },
        "sidebar-header-text-color": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "sidebar-unread-text": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "sidebar-text-active-border": {
          "$type": "color",
          "$value": "{color.foundation.blue.300}"
        },
        "sidebar-text-active-color": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "sidebar-team-background": {
          "$type": "color",
          "$value": "#162545",
          "$description": "Custom value darker than indigo-600"
        },
        "link-color": {
          "$type": "color",
          "$value": "{color.foundation.blue.500}"
        },
        "button-bg": {
          "$type": "color",
          "$value": "{color.foundation.blue.500}"
        },
        "button-color": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "online-indicator": {
          "$type": "color",
          "$value": "{color.foundation.green.500}"
        },
        "away-indicator": {
          "$type": "color",
          "$value": "{color.foundation.yellow.500}"
        },
        "dnd-indicator": {
          "$type": "color",
          "$value": "{color.foundation.red.400}"
        },
        "mention-bg": {
          "$type": "color",
          "$value": "{color.foundation.neutral.0}"
        },
        "mention-color": {
          "$type": "color",
          "$value": "{color.foundation.indigo.500}"
        },
        "mention-highlight-bg": {
          "$type": "color",
          "$value": "{color.foundation.yellow.200}"
        },
        "mention-highlight-link": {
          "$type": "color",
          "$value": "{color.foundation.neutral.1100}"
        },
        "error-text": {
          "$type": "color",
          "$value": "{color.foundation.red.400}"
        },
        "new-message-separator": {
          "$type": "color",
          "$value": "{color.foundation.yellow.600}"
        },
        "calls-bg": {
          "$type": "color",
          "$value": "{color.foundation.indigo.600}"
        }
      }
    }
  }
}
